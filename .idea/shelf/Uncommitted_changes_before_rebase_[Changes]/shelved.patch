+/*
+* Color
+*/
+
+
+/*
+* Background color
+*/
+
+.bg-base-color {
+    background-color: #f9461d !important;
+}
+
+
+/*
+* Border color
+*/
+
+.border-base-color {
+    border-color: #f9461d !important;
+}
+
+
+/*
+* Button color
+*/
+
+.btn-base-color {
+    background-color: #f9461d !important;
+}
+
+
+/*
+* Button color - hover
+*/
+
+.btn-hover-base-color:hover {
+    background-color: #f9461d !important;
+}
+
+
+/*
+* Features Block
+*/
+
+.features-block:before {
+    background: -moz-linear-gradient(top, rgba(249, 70, 29, 0) 0%, rgba(249, 70, 29, 0.9) 15%, rgba(249, 70, 29, 0.9) 85%, rgba(249, 70, 29, 0) 100%);
+    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, rgba(249, 70, 29, 0)), color-stop(15%, rgba(249, 70, 29, 0.9)), color-stop(85%, rgba(249, 70, 29, 0.9)), color-stop(100%, rgba(249, 70, 29, 0)));
+    background: -webkit-linear-gradient(top, rgba(249, 70, 29, 0) 0%, rgba(249, 70, 29, 0.9) 15%, rgba(249, 70, 29, 0.9) 85%, rgba(249, 70, 29, 0) 100%);
+    background: -o-linear-gradient(top, rgba(249, 70, 29, 0) 0%, rgba(249, 70, 29, 0.9) 15%, rgba(249, 70, 29, 0.9) 85%, rgba(249, 70, 29, 0) 100%);
+    background: -ms-linear-gradient(top, rgba(249, 70, 29, 0) 0%, rgba(249, 70, 29, 0.9) 15%, rgba(249, 70, 29, 0.9) 85%, rgba(249, 70, 29, 0) 100%);
+    background: linear-gradient(to bottom, rgba(249, 70, 29, 0) 0%, rgba(249, 70, 29, 0.9) 15%, rgba(249, 70, 29, 0.9) 85%, rgba(249, 70, 29, 0) 100%);
+}
+.features-block:hover .block-icon {
+    background-color: #f9461d;
+}
+.features-block:hover .block-icon i {
+    color: #ffffff;
+}
+.features-block .block-icon {
+    border: 1px solid #f9461d;
+}
+.features-block .block-icon i {
+    color: #f9461d;
+}
+
+
+/*
+* Flickity
+*/
+
+.carousel-custom .flickity-page-dots .dot.is-selected {
+    background: #f9461d;
+}
+
+
+/*
+* Heading style
+*/
+
+.title-underline-base-color {
+    border-color: #f9461d;
+}
+.title-underline-thick-base-color {
+    border-color: #f9461d;
+}
+.title-sideline-base-color:before {
+    background-color: #f9461d;
+}
+
+
+/*
+* Preloader
+*/
+
+.pace .pace-progress {
+    background-color: #f9461d;
+}
+
+
+/*
+* Text color
+*/
+
+.text-base-color {
+    color: #f9461d !important;
+}
+
+
+/*
+* Text color (hover)
+*/
+
+.text-hover-base-color:hover {
+    color: #f9461d !important;
+}
+
+
+/*
+* Timeline
+*/
+
+.timeline .timeline-item:before {
+    background-color: #f9461d !important;
+}
