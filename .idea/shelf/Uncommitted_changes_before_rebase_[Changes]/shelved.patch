+/*
+* Color
+*/
+
+
+/*
+* Background color
+*/
+
+.bg-base-color {
+    background-color: #aa2e85 !important;
+}
+
+
+/*
+* Border color
+*/
+
+.border-base-color {
+    border-color: #aa2e85 !important;
+}
+
+
+/*
+* Button color
+*/
+
+.btn-base-color {
+    background-color: #aa2e85 !important;
+}
+
+
+/*
+* Button color - hover
+*/
+
+.btn-hover-base-color:hover {
+    background-color: #aa2e85 !important;
+}
+
+
+/*
+* Features Block
+*/
+
+.features-block:before {
+    background: -moz-linear-gradient(top, rgba(170, 46, 133, 0) 0%, rgba(170, 46, 133, 0.9) 15%, rgba(170, 46, 133, 0.9) 85%, rgba(170, 46, 133, 0) 100%);
+    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, rgba(170, 46, 133, 0)), color-stop(15%, rgba(170, 46, 133, 0.9)), color-stop(85%, rgba(170, 46, 133, 0.9)), color-stop(100%, rgba(170, 46, 133, 0)));
+    background: -webkit-linear-gradient(top, rgba(170, 46, 133, 0) 0%, rgba(170, 46, 133, 0.9) 15%, rgba(170, 46, 133, 0.9) 85%, rgba(170, 46, 133, 0) 100%);
+    background: -o-linear-gradient(top, rgba(170, 46, 133, 0) 0%, rgba(170, 46, 133, 0.9) 15%, rgba(170, 46, 133, 0.9) 85%, rgba(170, 46, 133, 0) 100%);
+    background: -ms-linear-gradient(top, rgba(170, 46, 133, 0) 0%, rgba(170, 46, 133, 0.9) 15%, rgba(170, 46, 133, 0.9) 85%, rgba(170, 46, 133, 0) 100%);
+    background: linear-gradient(to bottom, rgba(170, 46, 133, 0) 0%, rgba(170, 46, 133, 0.9) 15%, rgba(170, 46, 133, 0.9) 85%, rgba(170, 46, 133, 0) 100%);
+}
+.features-block:hover .block-icon {
+    background-color: #aa2e85;
+}
+.features-block:hover .block-icon i {
+    color: #ffffff;
+}
+.features-block .block-icon {
+    border: 1px solid #aa2e85;
+}
+.features-block .block-icon i {
+    color: #aa2e85;
+}
+
+
+/*
+* Flickity
+*/
+
+.carousel-custom .flickity-page-dots .dot.is-selected {
+    background: #aa2e85;
+}
+
+
+/*
+* Heading style
+*/
+
+.title-underline-base-color {
+    border-color: #aa2e85;
+}
+.title-underline-thick-base-color {
+    border-color: #aa2e85;
+}
+.title-sideline-base-color:before {
+    background-color: #aa2e85;
+}
+
+
+/*
+* Preloader
+*/
+
+.pace .pace-progress {
+    background-color: #aa2e85;
+}
+
+
+/*
+* Text color
+*/
+
+.text-base-color {
+    color: #aa2e85 !important;
+}
+
+
+/*
+* Text color (hover)
+*/
+
+.text-hover-base-color:hover {
+    color: #aa2e85 !important;
+}
+
+
+/*
+* Timeline
+*/
+
+.timeline .timeline-item:before {
+    background-color: #aa2e85 !important;
+}
